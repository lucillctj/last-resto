{
    "extends": "@tsconfig/node18/tsconfig.json",
    "compilerOptions": {
        "target": "ES2022",
        "module": "es2022",
        "outDir": "dist",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src", "src/models"],
    "exclude": ["node_modules"]
  }